{
    "env": {
        "browser": true,
        "es2021": true
    },
   
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true, 
            "modules": true,
            "experimentalObjectRestSpread": true, 
            "exportDefaultFrom" : true
        },
    "babelOptions": {
      "parserOpts": {
        "plugins": [
                    "jsx"
                ]
            }
        }, 
        "ecmaVersion": 6,
        "sourceType": "module", 
        "requireConfigFile" : false
    },
    "parser": "@babel/eslint-parser",
    // "plugins": [
    //     "react"
    // ],
    "rules": {
        "radix": [
            "error",
            "as-needed"
        ],
        "no-underscore-dangle": [
            "error",
            {
                "enforceInMethodNames": false,
                "allowAfterThis": true,
                "allow": [
                    "_id",
                    "resource_" //realted to google
                ]
            }
        ],
        "no-const-assign": [
            "error"
        ],
        "no-var": [
            "error"
        ],
        "no-with": [
            "error"
        ],
        "init-declarations": [
            "warn",
            "always"
        ],
        "no-empty": [
            "warn",
            {
                "allowEmptyCatch": true
            }
        ],
        "no-empty-function": [
            "warn"
        ],
        "max-depth": [
            "error"
        ],
        "no-array-constructor": [
            "error"
        ],
        "no-case-declarations": [
            "error"
        ],
        "no-else-return": [
            "warn"
        ],
        "no-console": [
            "warn"
        ],
        "no-alert": [
            "warn"
        ],
        "camelcase": [
            "error",
            {
                "properties": "always",
                "allow": [
                    "access_token",
                    "dicom_path",
                    "frontend_dev",
                    "frontend_test",
                    "expiration_date",
                    "session_url",
                    "is_upgrade"
                ]
            }
        ],
        "no-dupe-class-members": [
            "error"
        ],
        "no-dupe-else-if": [
            "error"
        ],
        "no-dupe-keys": [
            "error"
        ],
        "no-duplicate-case": [
            "error"
        ],
        "no-duplicate-imports": [
            "error"
        ],
        "no-unexpected-multiline": [
            "error"
        ],
        "semi": [
            "error",
            "always",
            {
                "omitLastInOneLineBlock": true
            }
        ],
        "no-unmodified-loop-condition": [
            "error"
        ],
        "no-func-assign": [
            "error"
        ],
        "no-fallthrough": [
            "error"
        ],
        "function-call-argument-newline": [
            "error",
            "consistent"
        ],
        "no-unreachable": [
            "error"
        ],
        "no-loop-func": [
            "error"
        ],
        "no-multi-str": [
            "error"
        ],
        "no-nested-ternary": [
            "error"
        ],
        "no-param-reassign": [
            "warn"
        ],
        "no-return-assign": [
            "error"
        ],
        "no-use-before-define": [
            "error"
        ],
        "complexity": [
            "error",
            30
        ],
        "default-case": [
            "error"
        ],
        "eqeqeq": [
            "error"
        ],
        "max-lines": [
            "error",
            1000
        ],
        "max-nested-callbacks": [
            "error",
            2
        ],
        "no-return-await": [
            "warn"
        ],
        "require-await": [
            "warn"
        ],
        "no-dupe-args": [
            "error"
        ],
        "no-irregular-whitespace": [
            "error"
        ],
        "no-self-compare": [
            "warn"
        ],
        "no-template-curly-in-string": [
            "error"
        ],
        "no-script-url": [
            "error"
        ],
        "no-undefined": [
            "error"
        ],
        "require-yield": [
            "warn"
        ],
        "id-length": [
            "error",
            {
                "min": 3,
                "exceptions": [
                    "_",
                    "id",
                    "as",
                    "i",
                    "to",
                    "mt",
                    "ml",
                    "e",
                    "SR",
                    "m",
                    "t"
                ]
            }
        ]
        
       
    }
}
